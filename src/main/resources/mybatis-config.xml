<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>

		<typeAlias type="com.kh.mobiil.member.domain.Member"  	alias="Member"/>
		<typeAlias type="com.kh.mobiil.banner.domain.Banner" 	alias="Banner"/>
		<typeAlias type="com.kh.mobiil.chat.domain.ChatRoom" 	alias="ChatRoom"/>
		<typeAlias type="com.kh.mobiil.chat.domain.Chat" 	 	alias="Chat"/>
		<typeAlias type="com.kh.mobiil.partner.domain.Partner" 	alias="Partner"/>
		<typeAlias type="com.kh.mobiil.host.domain.Host" 		alias="Host"/>
		<typeAlias type="com.kh.mobiil.space.domain.Space" 		alias="Space"/>
		<typeAlias type="com.kh.mobiil.space.domain.SpaceImg" 	alias="SpaceImg"/>
		<typeAlias type="com.kh.mobiil.space.domain.Reservation"  alias="Reservation"/>
		<typeAlias type="com.kh.mobiil.review.domain.Review"  	  alias="Review"/>
		<typeAlias type="com.kh.mobiil.review.domain.ReviewImg" alias="ReviewImg"/>
		<typeAlias type="com.kh.mobiil.chat.domain.ChatSearchResult" alias="ChatSearchResult"/>
		<typeAlias type="com.kh.mobiil.space.domain.Heart" 		alias="Heart"/>
		

	</typeAliases>

	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/banner-mapper.xml"/>
		<mapper resource="mappers/chatRoom-mapper.xml"/>
		<mapper resource="mappers/chat-mapper.xml"/>
		<mapper resource="mappers/partner-mapper.xml"/>
		<mapper resource="mappers/host-mapper.xml"/>
		<mapper resource="mappers/space-mapper.xml"/>
		<mapper resource="mappers/review-mapper.xml"/>
	</mappers>

</configuration>